{
  "schemaVersion": 1,
  "id": "tputils",
  "version": "${version}",

  "name": "TP Utils",
  "description": "Teleport utility mod, making it much easier to get around.",
  "authors": [
    "Sjouwer",
    "Kyzeragon"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/tp-utils",
    "sources": "https://github.com/Sjouwer/tp-utils",
    "issues": "https://github.com/Sjouwer/tp-utils/issues"
  },

  "license": "GNU General Public License v3.0",
  "icon": "assets/tputils/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "io.github.sjouwer.tputils.TpUtils"
    ],
    "modmenu": [ "io.github.sjouwer.tputils.config.ModMenuApiImpl" ],
    "cotton-client-commands": ["io.github.sjouwer.tputils.Commands"]
},

  "depends": {
    "minecraft": "1.14.x",
    "fabricloader": ">=0.4.0",
    "fabric": ">=0.14.0",
    "cloth-config2": ">=1.0.0",
    "autoconfig1u": ">=1.0.0"
  }
